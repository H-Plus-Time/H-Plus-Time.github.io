{
  "entrypoint": "index.html",
  "shell": "src/showcase-app.html",
  "fragments": [
    "src/showcase-home.html",
    "src/showcase-portfolio.html",
    "src/showcase-til.html",
    "src/showcase-resume.html",
    "src/showcase-view404.html"
  ],
  "sources": [
   "src/**/*",
   "images/**/*",
   "data/**/*",
   "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-loader.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "preset": "es6-bundled",
    "addPushManifest": false,
    "swPrecacheConfig": "sw-precache-config.js",
    "name": "default"
  }]
}
